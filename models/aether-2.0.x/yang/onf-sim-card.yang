submodule onf-sim-card {
  belongs-to onf-enterprise {
    prefix ent;
  }

  import onf-aether-types {
    prefix at;
  }

  organization
    "Open Networking Foundation.";
  contact
    "Scott Baker";
  description
    "A Aether Device...";

  revision 2022-01-19 {
    description
      "An Aether Device";
    reference
      "RFC 6087";
  }

  typedef sim-card-id {
    type at:aether-identifier;
    description
      "The typedef for device-id";
  }

  grouping sim-card {
    description
      "The top level container";
    list sim-card {
      key "sim-id";
      unique "iccid";
      description
        "List of sim cards";
      leaf sim-id {
        type sim-card-id;
        description
          "ID for this sim card.";
      }
      uses at:desc-display-name;
      leaf iccid {
        type at:iccid;
        description
          "ICCID for this sim card";
      }
      leaf imsi {
        type at:imsi;
        description
          "IMSI for this sim card";
      }
    }
  }
}
